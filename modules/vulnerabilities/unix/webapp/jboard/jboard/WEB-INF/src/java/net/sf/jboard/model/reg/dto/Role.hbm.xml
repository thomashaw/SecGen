<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping
    PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">

<hibernate-mapping>

 <class name="net.sf.jboard.model.reg.dto.Role" table="role">
  <id name="id" type="long" unsaved-value="0" column="id">
   	<generator class="native">
   		<param name="sequence">role_seq</param>
   	</generator>
   </id>        	
   <property name="roleName" column="rolename" />	
   
   <set name="users" table="user_role" inverse="true" >	
   <key column="role_id"  />
   <many-to-many class="net.sf.jboard.model.reg.dto.User" column="user_id" />
   </set>
   
 </class>

</hibernate-mapping>